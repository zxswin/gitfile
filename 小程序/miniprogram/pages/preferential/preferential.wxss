.display-flex {
  display: flex;
  justify-content: center;
  align-items: center;
}
.block-flex {
  display: flex;
  justify-content: center;
  align-items: center;
}
.product-com-box {
  padding: 0 10rpx;
}
.prefer-contain {
  padding: 20rpx;
  padding-bottom: 200rpx;
}
.prefer-title {
  font-size: 36rpx;
  color: #000;
  font-weight: bold;
  margin: 20rpx 0 40rpx 0;
}
/** 套餐组合  */
.prefer-product {
  position: relative;
  padding-bottom: 10rpx;
  box-shadow: 0 20rpx 20rpx -10rpx #888888;
  margin-bottom: 30rpx;
}
.prefer-product-img {
  width: 100%;
  padding-bottom: 20rpx;
}
.prefer-product-imgwidth {
  width: 100%;
  height: 360rpx;
  display: block;
}
.prefer-product-tag {
  position: absolute;
  top: 0;
  right: 0;
  padding: 10rpx 20rpx;
  color: #fff;
  background: rgba(0, 0, 0, 0.5);
  font-size: 32rpx;
}
.prefer-product-label {
  padding-bottom: 20rpx;
}
.product-label {
  color: #fff;
  border-radius: 10rpx;
  font-size: 28rpx;
  display: inline-block;
  padding: 8rpx 16rpx;
  margin-right: 16rpx;
}
.product-label-text {
  background: #07c160;
}
.product-label-coupon {
  background: #cc004d;
}

.prefer-product-title {
  padding-bottom: 20rpx;
}
.product-title {
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 100%;
  overflow: hidden;
}
.product-title-maintit {
  font-size: 32rpx;
  color: #000;
  padding-bottom: 16rpx;
}
.product-title-subtit {
  font-size: 28rpx;
  color: #888888;
}
.prefer-product-price {
  padding-bottom: 10rpx;
  justify-content: space-between;
}
.product-price {
  display: inline-block;
  font-size: 40rpx;
  margin-right: 20rpx;
}
.product-price-final {
  color: #07c160;
}
.product-price-discount {
  color: #888888;
}
.product-price-shoping {
  width: 60rpx;
  height: 60rpx;
  background: url('data:image/png;base64,')
    no-repeat;
  background-size: cover;
}

/** 购物车挂件  */
.shop-widgets {
  position: fixed;
  bottom: 100rpx;
  left: 40rpx;
  width: 100rpx;
  height: 100rpx;
  border: 2rpx solid #07c160;
  background: #fff;
  border-radius: 50%;
  z-index: 100;
}
.shop-widgets-icon {
  width: 60rpx;
  height: 60rpx;
}
