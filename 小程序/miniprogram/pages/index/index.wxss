/**index.wxss**/
.display-flex {
  display: flex;
  justify-content: center;
  align-items: center;
}
.i-top {
  height: 90rpx;
  background: #fff;
  border-bottom: 2rpx solid #f6f6f4;
  font-size: 38rpx;
  color: #07c160;
  display: flex;
  justify-content: center;
  align-items: center;
  font-weight: bold;
  box-shadow: 2rpx 2rpx 20rpx #888888;
  z-index: 10;
  position: relative;
}

page {
  overflow-y: hidden;
}

.i-content {
  position: relative;
  display: flex;
  height: calc(100vh - 90rpx);
}

.i-left {
  width: 20%;
}
.i-right {
  width: 80%;
  background: #fff;
  margin-left: 10rpx;
}
.scroll-view {
  height: 100%;
}

.scroll-view-item {
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  border-left: 10rpx solid transparent;
}
.menu-item-l {
  height: 150rpx;
  background: #f6f6f4;
  display: flex;
  justify-content: center;
  align-items: center;
}

.menu-l-active {
  border-left: 10rpx solid #07c160;
  background: #fff;
}

.list-item {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 10rpx;
  padding-bottom: 20rpx;
  border-bottom: 2rpx solid #f6f6f4;
}

.list-item:nth-last-of-type(1) {
  padding-bottom: 200rpx;
}

.list-item-l {
  width: 25%;
  position: relative;
}
.show-sellout {
  position: absolute;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.5);
  color: #fff;
  font-size: 28rpx;
  width: 100%;
  padding: 8rpx 0;
  text-align: center;
}
.list-item-r {
  width: 75%;
  display: flex;
  justify-content: center;
  align-items: center;
  flex-direction: column;
  padding: 0 10rpx;
}
.list-item-r view {
  margin-top: 10rpx;
}
.item-l-img {
  width: 100%;
  height: 200rpx;
}

.item-r-text {
  color: #000;
  font-size: 36rpx;
}

.item-r-des {
  color: #888888;
  font-size: 28rpx;
}
.item-r-pri {
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  width: 100%;
}
.pri-btn {
  position: relative;
  overflow: visible;
}
.i-count {
  position: absolute;
  top: -30rpx;
  right: -10rpx;
  width: 40rpx;
  height: 40rpx;
  border: 4rpx solid #fff;
  border-radius: 50%;
  background: #07c160;
  display: flex;
  justify-content: center;
  align-items: center;
}
/** 结算按钮  */
.bottom-pay {
  position: absolute;
  background: #e7e7e7;
  left: 0;
  bottom: -300rpx;
  /* bottom: 0; */
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}

.shopping-cart {
  position: absolute;
  top: -20%;
  left: 40rpx;
  background: #fff;
  width: 35%;
  height: 100%;
  border-radius: 100rpx;
  display: flex;
  justify-content: space-evenly;
  align-items: center;
}

.shop_icon {
  width: 25%;
  height: 100%;
}
.shop_title {
  font-size: 36rpx;
}
.shop_count {
  position: absolute;
  top: -20rpx;
  right: 0;
  width: 60rpx;
  height: 60rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  background: #07c160;
  border-radius: 50%;
  font-size: 32rpx;
}
.pay-price {
  font-size: 36rpx;
  margin-right: 20rpx;
}

.pay-button {
  width: 35%;
  background: #07c160;
  color: #fff;
  font-size: 40rpx;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}

/** 购物结算清单  */
.settle-list {
  width: 100%;
  padding-bottom: 180rpx;
  min-height: 200rpx;
  background: #fff;
  position: absolute;
  left: 0;
  bottom: -800rpx;
}

/** 清空购物车  */
.settle-mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background: rgba(0, 0, 0, 0.5);
  top: 0;
  left: 0;
}
.list-clear-icon {
  padding: 20rpx;
  background: #f6f7f9;
}
.clear-shop-text {
  font-size: 30rpx;
  color: #686868;
  background: url('data:image/png;base64,')
    no-repeat;
  background-size: contain;
  padding-left: 50rpx;
  line-height: 30rpx;
}
/** 购物车订单  */
.order-settle {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 20rpx;
  border-bottom: 2rpx solid #e4e4e4;
}

.order-settle-dsc {
  width: 45%;
  font-size: 28rpx;
  color: #a2a2a2;
}
.order-dsc-title {
  font-size: 30rpx;
  color: #000;
  padding-bottom: 10rpx;
}
.order-settle-price {
  color: #000;
  font-size: 36rpx;
}

.order-op-r {
  width: 40%;
}

/** 滚动列表去除滚动条  */
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}

/** 订单结算列表滚动条设置  */
.settle-scroll-view {
  max-height: 600rpx;
}

/** 购物车挂件  */
.shop-widgets {
  position: fixed;
  bottom: 100rpx;
  left: 40rpx;
  width: 100rpx;
  height: 100rpx;
  border: 2rpx solid #07c160;
  background: #fff;
  border-radius: 50%;
  z-index: 100;
}
.shop-widgets-icon {
  width: 60rpx;
  height: 60rpx;
}
